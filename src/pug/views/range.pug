            +partialEvent({ vspan: 1, txtColor: 4, bgColor: 4, bdColor: 4, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 3, bgColor: 3, bdColor: 3, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 2, bgColor: 2, bdColor: 2, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 1, bgColor: 1, bdColor: 1, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 2, bgColor: 2, bdColor: 2, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 3, bgColor: 3, bdColor: 3, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
