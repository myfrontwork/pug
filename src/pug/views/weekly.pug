// [모드] 주간
.mode-view.mv-cols.mv-cols-week(data-view="cols-view")
  // 상단 고정 영역
  .mv-cols-head(data-part="cols-main-top")
    // 스크롤바 영역 이외
    .mv-cols-head-content
      table.mv-cols-head-table
        tbody
          // 일요일 ~ 토요일
          tr.mv-cols-head-dayname
            th.mv-cols-head-dayname-offset
            th.mv-cols-head-dayname-item 13 (일)
            th.mv-cols-head-dayname-item 14 (월)
            th.mv-cols-head-dayname-item 15 (화)
            th.mv-cols-head-dayname-item.date-today 16 (수)
            th.mv-cols-head-dayname-item 17 (목)
            th.mv-cols-head-dayname-item 18 (금)
            th.mv-cols-head-dayname-item 19 (토)
          // 종일일정
          tr.mv-cols-head-daylong
            td.mv-cols-head-daylong-offset
              span.label 종일
            td(colspan="7")
              .mv-cols-head-daylong-group
                // 종일일정: 배경
                .mv-cols-head-daylong-bg
                  table.mv-cols-head-daylong-bg-table
                    tbody
                        td.mv-cols-head-daylong-bg-item
                        td.mv-cols-head-daylong-bg-item
                        td.mv-cols-head-daylong-bg-item
                        td.mv-cols-head-daylong-bg-item.date-today
                        td.mv-cols-head-daylong-bg-item
                        td.mv-cols-head-daylong-bg-item
                        td.mv-cols-head-daylong-bg-item
                // 종일일정: 내용
                .mv-cols-head-daylong-data
                  table.mv-cols-head-daylong-data-table
                    tbody
                      tr
                        td.mv-cols-head-daylong-data-item(colspan="4")
                          +daylongEvent({ txtColor: 4, bgColor: 4, bdColor: 4, daylong: true, name: "일정내용" })
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                      tr
                        td.mv-cols-head-daylong-data-item
                          +daylongEvent({ txtColor: 6, bgColor: 6, bdColor: 6, daylong: true, name: "일정내용" })
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
                        td.mv-cols-head-daylong-data-item
    // 스크롤바 영역
    .mv-cols-head-scroll
      .placeholder
    // IE 대응용 백그라운드 분리 hack
    .mv-cols-head-bg(role="presentation")
  // /상단 고정 영역

  // 시간별 일정 영역(스크롤 가능)
  .mv-cols-body(data-part="cols-main-body")
    table.mv-cols-body-grid
      td.mv-cols-body-grid-offset
        .mv-cols-body-grid-timeline
          +colsTimelineCell("12시", "오전")
          +colsTimelineCell("1시")
          +colsTimelineCell("2시")
          +colsTimelineCell("3시")
          +colsTimelineCell("4시")
          +colsTimelineCell("5시")
          +colsTimelineCell("6시")
          +colsTimelineCell("7시")
          +colsTimelineCell("8시")
          +colsTimelineCell("9시")
          +colsTimelineCell("10시")
          +colsTimelineCell("11시")
          +colsTimelineCell("12시", "오후")
          +colsTimelineCell("1시")
          +colsTimelineCell("2시")
          +colsTimelineCell("3시")
          +colsTimelineCell("4시")
          +colsTimelineCell("5시")
          +colsTimelineCell("6시")
          +colsTimelineCell("7시")
          +colsTimelineCell("8시")
          +colsTimelineCell("9시")
          +colsTimelineCell("10시")
          +colsTimelineCell("11시")
      // 일
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
      // /일
      // 월
      //- (한컴).sch-group 수정
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
          // top 값: (19px * number of grid rows), (한컴) 수정
          .sch-group.sch-time.row-2(style=`top: ${19 * 18}px;  left: 0%; width: 10%; z-index: ;`)
            +partialEvent({ txtColor: 10, bgColor: 10, bdColor: 10, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true, pst: true })
          .sch-group.sch-time.row-2(style=`top: ${19 * 18}px;  left: 10%; width: 10%; z-index: ;`)
            +partialEvent({ txtColor: 9, bgColor: 9, bdColor: 9, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true, pst: true })
          .sch-group.sch-time.row-5(style=`top: ${19 * 18}px; left: 20%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 8, bgColor: 8, bdColor: 8, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true, pst: true })
          .sch-group.sch-time.row-5(style=`top: ${19 * 18}px; left: 30%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 7, bgColor: 7, bdColor: 7, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true, pst: true })
          .sch-group.sch-time.row-5(style=`top: ${19 * 18}px; left: 40%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 6, bgColor: 6, bdColor: 6, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-13(style=`top: ${19 * 18}px; left: 50%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 5, bgColor: 5, bdColor: 5, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-13(style=`top: ${19 * 18}px; left: 60%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 4, bgColor: 4, bdColor: 4, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-13(style=`top: ${19 * 18}px; left: 70%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 3, bgColor: 3, bdColor: 3, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-10(style=`top: ${19 * 18}px; left: 80%; width: 10%;  z-index: ;`)
            +partialEvent({ txtColor: 2, bgColor: 2, bdColor: 2, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-2(style=`top: ${19 * 18}px; left: 90%; width: 10%; z-index: ;`)
            +partialEvent({ txtColor: 1, bgColor: 1, bdColor: 1, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })

          .sch-group.sch-time.row-2(style=`top: ${19 * 21}px; left: 20%; width: 80%; z-index: ;`)
            +partialEvent({ txtColor: 5, bgColor: 5, bdColor: 5, break: true, time: "오전 10:00 ~ 오후 1:30", name: "일정내용", alarm: true, attached: true, repeat: true, drgic:true })
      // /월
      // 화
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 49;
          while (--i)
            - var selected = 30 < i && i < 48;  // 10시에서 1시 사이를 선택
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell", class=selected ? "time-selected" : null) 
      // /화
      // 수
      //- (한컴).sch-group 수정
      td.mv-cols-body-grid-col.date-today
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell") 
          // top 값: (19px * rowIndex - 1), (한컴) 수정
          .sch-group.sch-time.row-3(style=`top: ${19 * 18}px;  left: 0%; width: 33.3333333%; z-index: ;`)
            +partialEvent({ txtColor: 10, bgColor: 10, bdColor: 10, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-3(style=`top: ${19 * 18}px;  left: 33.3333333%; width: 33.3333333%; z-index: ;`)
            +partialEvent({ txtColor: 9, bgColor: 9, bdColor: 9, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })
          .sch-group.sch-time.row-3(style=`top: ${19 * 18}px;  left: 66.666666%; width: 33.3333333%; z-index: ;`)
            +partialEvent({ txtColor: 8, bgColor: 8, bdColor: 8, break: true, time: "오전 9:00 ~ 오전 10:00", name: "일정내용", drgic:true })

          
          // 현재 시각 표시
          .now-indicator(style="top: 280px;")
      // /수
      // 목
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
      // /목
      // 금
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
      // /금
      // 토
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
      // /토
  // /시간별 일정 영역(스크롤 가능)
// /[모드] 주간