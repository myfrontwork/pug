            +partialEvent({ vspan: 1, txtColor: 9, bgColor: 9, bdColor: 9, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 10, bgColor: 10, bdColor: 10, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 9, bgColor: 9, bdColor: 9, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 8, bgColor: 8, bdColor: 8, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 7, bgColor: 7, bdColor: 7, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 6, bgColor: 6, bdColor: 6, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 5, bgColor: 5, bdColor: 5, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 4, bgColor: 4, bdColor: 4, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 3, bgColor: 3, bdColor: 3, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 2, bgColor: 2, bdColor: 2, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
            +partialEvent({ vspan: 1, txtColor: 1, bgColor: 1, bdColor: 1, break: true, time: "오후 1:00 ~ 오후 1:30", name: "일정내용" })
          // 현재 시각 표시
          .now-indicator(style="top: 280px;")
      // /수
      // 목
      td.mv-cols-body-grid-col
        .mv-cols-body-grid-group
          - var i = 48;
          while (--i)
            .mv-cols-body-grid-cell.js-sch-cell(role="gridcell")
      // /목
  // /시간별 일정 영역(스크롤 가능)
